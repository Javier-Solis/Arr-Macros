{
    "name": "voicingewe/array-macros",
    "description": "A set of useful Laravel array macros",
    "keywords": [
        "voicingewe",
        "laravel",
        "array",
        "macros"
    ],
    "homepage": "https://github.com/voicingewe/array-macros",
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Solis",
            "email": "voicingewe@outlook.com",
            "homepage": "https://github.com/voicingewe",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "6.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "VoicingEwe\\ArrayMacros\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VoicingEwe\\ArrayMacros\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VoicingEwe\\ArrayMacros\\ArrayMacroServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
