{
  "name": "jvrsolis/array-macros",
  "description": "A set of useful Laravel array macros",
  "keywords": [
    "jvrsolis",
    "laravel",
    "array",
    "macros"
  ],
  "homepage": "https://github.com/jvrsolis/array-macros",
  "license": "MIT",
  "authors": [
    {
      "name": "Javier Solis",
      "email": "jvrsolis@outlook.com",
      "homepage": "https://github.com/jvrsolis",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "laravel/framework": "~5.6.0|~5.7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "JvrSolis\\ArrayMacros\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JvrSolis\\ArrayMacros\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "JvrSolis\\ArrayMacros\\ArrayMacroServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}